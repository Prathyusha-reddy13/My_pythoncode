n = int(input("enter number"))
i =0
sum = 0
while i<n:
    i = i+1
    sum = sum+i
print("The sum of 'n' natural numbers is ",sum)
#Another method
n =int(input("enter the numbers upto which you need sum"))
result = (n*(n+1))//2
print("The sum of 'n' natural numbers is ",result)
#another method using for loop
n =int(input("enter the numbers upto which you need sum"))
sum =0
for i in range(1,n+1):
    sum = sum+i
print("The sum of 'n' natural numbers is ",sum)
